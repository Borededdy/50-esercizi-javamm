int hIndex(int[] a) {
    for (int h = a.length; h > 0; h--) {
        
        int contatore = 0;
        
        for (int i = 0; i < a.length; i++) {
            if (a[i] >= h) {
                contatore++;
            }
        }
        
        if (contatore >= h) {
            return h;
        }
    }
    
    return 0;
}
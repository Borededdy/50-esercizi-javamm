boolean amicable(int a, int b) {
	if(calcolaSommaDivisori(a) == b && calcolaSommaDivisori(b) == a) return true;
	return false;
}

int calcolaSommaDivisori(int n) {
    int sum = 0;

    for (int i = 1; i <= n / 2; i++) {
        if (n % i == 0) {
            sum += i;
        }
    }
    return sum;
}